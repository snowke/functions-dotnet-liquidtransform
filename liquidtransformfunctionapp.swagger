  - http
paths:
  '/api/liquidtransformer/{LiquidTransformFilename}':
    post:
      operationId: '/api/liquidtransformer/{LiquidTransformFilename}/post'
      consumes:
      - "application/xml"
      - "application/json"
      produces:
      - "application/xml"
      - "application/json"
      - "text/plain"
      parameters:
        - in: path
          name: LiquidTransformFilename
          required: true
          type: string
        - in: body
          name: Content
          required: true
          schema:
            type: object
        - in: header
          name: Accept
          required: true
          type: string
        - in: header
          name: Content-Type
          required: true
          type: string
        - in: header
          name: X-CSV-Column-Delimiter
          required: false
          type: string            
      responses:
        '200':
          description: Success operation
      security:
        - apikeyQuery: []
definitions: {}
securityDefinitions:
  apikeyQuery:
    type: apiKey
    name: code
    in: query
